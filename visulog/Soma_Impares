algoritmo "Soma_impares"

var
 x, y, soma, i, troca: Inteiro
 
inicio
      Escreval("Digite dois numeros: ")
      leia(x)
      leia(y)
      
      Se x > y entao
         troca <- x
         x <- y
         y <- troca
      fimse
      
      soma <- 0

      Para i de x+1 ate y-1 faca
           se (i % 2) <> 0  entao
              soma <- soma + i
           fimse
      fimpara
      
      escreva("Soma dos impares = ", soma)
      
fimalgoritmo