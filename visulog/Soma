var
        x,y : real
        soma : real
        
inicio
      Escreva("Digite o valor de x: ")
      leia(x)
      escreval
      Escreva("Digite o valor de y: ")
      leia(y)
      escreval

      soma <- (x + y)
      
      escreval("Soma = ",soma)
